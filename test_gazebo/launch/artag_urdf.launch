<launch>
	<arg name="x" default="0"/>
	<arg name="y" default="0"/>
	<arg name="z" default="0"/>
	<arg name="roll" default="0"/>
	<arg name="pitch" default="0"/>
	<arg name="yaw" default="0"/>

	<arg name="urdf_artag_file" default=""/>
	<arg name="tag_name" default=""/>

	<param name="artag_description" command="cat $(arg urdf_artag_file)" />

	<node name="$(arg tag_name)_urdf_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen" args="-urdf -x $(arg x) -y $(arg y) -z $(arg z) -R $(arg roll) -P $(arg pitch) -Y $(arg yaw) -model $(arg tag_name) -param artag_description" />

</launch>
